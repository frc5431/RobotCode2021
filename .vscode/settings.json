{
    "java.configuration.updateBuildConfiguration": "automatic",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "bin/": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "wpilib.autoStartRioLog": false,
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.useTabStops": true,
    "editor.insertSpaces": true
}